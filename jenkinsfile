pipeline{
    agent any 
    tools {
        nodejs 'nodejs'
    }
    stages {
        stage ('run'){
            step{
                sh 'npm install'
            }
        }
        stage {
            step{
                sh 'npm test' 
                 echo "No tests defined"
            }
        }
        stage {
            step('build'){
                sh 'npm run build'  
            }
        }
    }
}